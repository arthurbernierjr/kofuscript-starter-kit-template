_ = require "underscore"
app = document.getElementById "app"

num :: Int = 89990
present num

name =
  value: 'arthur'
  update: (newName)->
    @value = newName
    @render()
  render: -> app.innerHTML = """<h1 onclick="promptForName()">You so rock from #{@value}</h1> """


window.promptForName = (e)->
  newName = window.prompt "Lets Change Your Name"
  name.update newName

name.render()
